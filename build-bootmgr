rm -f *.o bootmgr
nasm -g -f elf32  -o bootloader.o  yy_bootloader/bootloader.s
gcc -g -march=i386 -m32 -ffreestanding -fno-pie  -c yy_bootloader/loadkernel.c 
#ld -g -melf_i386 -N -Ttext 0x8000 -Ttext-segment 0x9000 --oformat binary -Map mymap --verbose -o bootmgr bootloader.o loadkernel.o
ld -melf_i386 -N -Ttext 0x8000 -Map mymap -o bootmgr.elf  bootloader.o loadkernel.o
objcopy -O binary bootmgr.elf bootmgr
dd conv=notrunc if=bootmgr of=c.img seek=1
hexdump -C bootmgr

