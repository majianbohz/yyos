rm -f *.o kernel 
nasm -g -f elf32  -o print.o  common/print.s
gcc -g -march=i386 -m32 -ffreestanding -fno-pie  -c yy_kernel/kernel.c 
ld -melf_i386 -N -Ttext 0x100000 -Map mapkernel -o kernel.elf  kernel.o  print.o
objcopy -O binary kernel.elf kernel
cp -f kernel cimg/
hexdump -C kernel
echo "ls -l cimg/"
ls -l cimg
